  Stage1:
  - branch: 1
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\u2026"
  - branch: 1
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uB204\uAC00 \uC774\uB7F0 \uACF3\uC5D0 \uBAA8\uB2E5\uBD88\uC744 \uD53C\uC6CC
      \uB193\uC558\uC744\uAE4C?"
  - branch: 2
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uBC29\uAE08 \uC800\uAE30\uC11C \uBB34\uC2A8 \uC18C\uB9AC\uAC00 \uB09C
      \uAC83 \uAC19\uC544!"
  - branch: 2
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uC124\uB9C8\u2026 \uC544\uAE4C \uADF8 \uACE0\uC591\uC774?"
  - branch: 3
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC5D0\uAD6C\uAD6C \uD798\uB4E4\uB2E4"
  - branch: 4
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC73C\uC545! \uAE5C\uC9DD\uC774\uC57C!"
  - branch: 4
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uB108 \uB204\uAD6C\uC57C? \uAE5C\uC9DD \uB180\uB790\uC796\uC544!"
  - branch: 4
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uBBF8\uC548\uD574\n\uC77C\uBD80\uB85C \uADF8\uB7F0 \uAC74 \uC544\uB2C8\uC57C"
  - branch: 4
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uB09C \uB2EC\uC774\uB77C\uACE0 \uD574\n\uB3D9\uC0DD\uC744 \uCC3E\uACE0
      \uC788\uC5B4\n\uD639\uC2DC \uC624\uBA74\uC11C \uD558\uC580 \uC950\uB97C \uBCF4\uC9C0
      \uBABB\uD588\uB2C8?"
  - branch: 4
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC950? \uB108\uBCF4\uB2E4 \uB354 \uCABC\uADF8\uB9E3\uB2E8 \uB9D0\uC774\uC57C?"
  - branch: 4
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC798 \uBAA8\uB974\uACA0\uB294 \uAC78\u2026\n\uC2E4\uC740 \uB9D0\uC774\uC57C"
  - branch: 4
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC544\uAE4C \uC800\uCABD\uC5D0 \uAC80\uC740 \uACE0\uC591\uC774\uAC00
      \uC788\uAE38\uB798\n\uD5D0\uB808\uBC8C\uB5A1 \uB3C4\uB9DD \uC654\uAC78\uB791\n\uC8FC\uC704\uB97C
      \uBCFC \uC2DC\uAC04\uC774 \uC5C6\uC5C8\uC5B4"
  - branch: 4
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uB9DE\uC544! \uADF8 \uAC80\uC740 \uACE0\uC591\uC774\uAC00 \uB0B4 \uB3D9\uC0DD\uC744
      \uB370\uB9AC\uACE0 \uAC00\uBC84\uB838\uC5B4\n\uB09C \uB3D9\uC0DD\uC744 \uCC3E\uC73C\uB7EC
      \uC5EC\uAE30\uAE4C\uC9C0 \uB6F0\uC5B4\uC628 \uAC70\uC57C"
  - branch: 5
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC5B4\uD734 \uB108\uB3C4 \uCC38 \uACE0\uC0DD\uC774 \uB9CE\uAD6C\uB098\n\uC5EC\uAE30\uC11C
      \uC880 \uC26C\uC5C8\uB2E4\uAC00 \uAC00"
  - branch: 5
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC694 \uC55E\uC740 \uAC1C\uC6B8\uC774\uB2C8\uAE4C\n\uADF8 \uAC80\uC740
      \uACE0\uC591\uC774 \uB140\uC11D\uB3C4 \uADF8\uB9AC \uBA40\uB9AC \uAC00\uC9C0\uB294
      \uBABB\uD588\uC744\uAC70\uC57C"
  - branch: 5
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uADF8\uB798\uC57C\uACA0\uB2E4 \uACE0\uB9C8\uC6CC!"
  - branch: 5
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uADF8\uB9AC\uACE0 \uC774\uAC74 \uB098\uB9CC \uC54C\uACE0 \uC788\uB294
      \uBE44\uBC00\uC778\uB370\u2026\n\uC800 \uC55E\uC5D0 \uBBFC\uB4E4\uB808\uAC00
      \uC794\uB729 \uD53C\uC5B4\uC788\uAC78\uB791\n\uADF8\uAC78 \uD0C0\uBA74 \uB0A0\uC544\uC11C
      \uAC1C\uC6B8\uC744 \uAC74\uB110 \uC218 \uC788\uC744\uAC70\uC57C"
  - branch: 5
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uB0A0\uC544\uC11C? \uADF8\uAC8C \uC815\uB9D0 \uAC00\uB2A5\uD574?"
  - branch: 5
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uC815\uB9D0\uC774\uB77C\uB2C8\uAE50!\n\uC0AC\uC2E4 \uB098\uB3C4 \uC61B\uB0A0\uC5D0
      \uADF8\uB807\uAC8C \uB2E4\uB154\uC5C8\uB294\uB370\n\uC694\uC998 \uC870\uAE08
      \uC0B4\uC774 \uCA84\uC11C \uC798 \uC548\uB418\uB354\uB77C\uACE0"
  - branch: 5
    speakerIndex: 1
    name: "\uB2E4\uB78C"
    message: "\uB108\uAC19\uC774 \uCABC\uADF8\uB9CC \uC560\uB4E4\uC740 \uCDA9\uBD84\uD788
      \uAC00\uB2A5\uD560 \uAC70\uC57C"
  - branch: 5
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uADF8\uB807\uAD6C\uB098 \uC54C\uB824\uC918\uC11C \uC815\uB9D0 \uACE0\uB9C8\uC6CC!"
  - branch: 5
    speakerIndex: 0
    name: "\uB2EC\uC774"
    message: "\uADF8\u2026 \uADF8\uB7FC \uC54C\uACA0\uC5B4!"
